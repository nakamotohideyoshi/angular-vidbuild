{
  "name": "VidBuild",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --environment=dev --aot",
    "build-qa": "ng build --environment=qa --aot",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "deploy-prod": "firebase use prod && npm run build-prod && npm run sw && firebase deploy --only hosting",
    "deploy-qa": "firebase use qa && npm run build-qa && npm run sw && firebase deploy --only hosting",
    "deploy-dev": "firebase use dev && npm run build-dev && npm run sw && firebase deploy --only hosting",
    "static-serve": "npm run build && npm run sw && cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "@angular/http": "^5.0.1",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/platform-server": "^5.0.1",
    "@angular/router": "^5.0.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "angular-animation-lib": "^1.0.0",
    "angular2-infinite-scroll": "^0.3.5",
    "angularfire": "^2.3.0",
    "angularfire2": "^5.0.0-rc.0",
    "angularfire2-offline": "^4.3.1",
    "aws-sdk": "^2.162.0",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "firebase": "^4.6.1",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "^4.7.0",
    "glyphicons": "^0.2.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.0",
    "json-stringify-safe": "5.0.1",
    "jszip": "^3.1.4",
    "ng2-search-filter": "^0.3.1",
    "ngx-bootstrap": "^1.9.3",
    "ngx-infinite-scroll": "^0.6.1",
    "node-sass": "^4.6.0",
    "push.js": "^1.0.5",
    "rxjs": "^5.5.2",
    "tether": "^1.4.1",
    "uuid": "^3.1.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5",
    "@angular/compiler-cli": "^5.0.1",
    "@angular/language-service": "^4.4.6",
    "@types/crypto-js": "^3.1.37",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.92",
    "codelyzer": "~3.1.1",
    "firebase-server": "^0.11.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
